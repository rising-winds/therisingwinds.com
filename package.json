{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "npx --node-arg '-r esm' gatsby build",
    "develop": "npx --node-arg '-r esm' gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npx --node-arg '-r esm' npm run develop",
    "serve": "npx --node-arg '-r esm' gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "dependencies": {
    "esm": "^3.2.25",
    "gatsby": "^2.13.31",
    "gatsby-plugin-netlify-cms": "^4.1.6",
    "gatsby-source-filesystem": "^2.1.6",
    "gatsby-transformer-remark": "^2.6.9",
    "netlify-cms-app": "^2.9.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "moment": "^2.24.0",
    "prettier": "^1.18.2",
    "storybook": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://russellskaggs:CCmTwYHXF8w3EZrcATzE@bitbucket.org/orderofcybermancy/rising-winds-website.git"
  }
}
